# Uncomment the next line to define a global platform for your project
# platform :ios, '9.0'

target 'VDO' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!

  # Pods for VDO
  pod 'Firebase/Core'
  pod 'Firebase/Auth'
  
  # UI Auth firebase
  pod 'FirebaseUI'
  
  
  target 'VDOTests' do
    inherit! :search_paths
    # Pods for testing
    pod 'Firebase/Core'
    pod 'Firebase/Auth'
    
    # UI Auth firebase
    pod 'FirebaseUI'
  end

  target 'VDOUITests' do
    inherit! :search_paths
    # Pods for testing
  end

end
